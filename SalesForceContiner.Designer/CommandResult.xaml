<Window x:Class="UiPathTeam.Salesforce.Marketing_Cloud.Activities.CommandResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Command Result" Height="277" Width="369" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Margin="2" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">

    <Grid Margin="0,0,0,0" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="121*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lbStatus" Content="Label" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-2.632,-0.192" Width="365" FontWeight="Bold" FontSize="18" Height="34" Grid.ColumnSpan="2" AutomationProperties.IsOffscreenBehavior="Onscreen" HorizontalContentAlignment="Center"/>
        <Label x:Name="lbResponse" Content="Label" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" FontWeight="Bold" Foreground="#FF1C661F" Width="326" Height="26"/>
        <TextBox x:Name="tbResponse" HorizontalAlignment="Center" Height="81" Margin="0,65,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="326" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto" />
        <Label x:Name="lbID" Content="Label" HorizontalAlignment="Center" Margin="0,0,0,70" VerticalAlignment="Bottom" RenderTransformOrigin="0.211,0.538" FontWeight="Bold" Foreground="#FF1825AC" Width="326" Height="26"/>
        <TextBox x:Name="tbID" HorizontalAlignment="Center" Height="23" Margin="0,0,0,50" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Width="326"/>
        <Button x:Name="btDelete" Content="Revert Command" HorizontalAlignment="Left" Margin="30,0,0,15" VerticalAlignment="Bottom" Width="135" Click="btDelete_Click" Height="25"/>
        <Button x:Name="btContinue" Content="Continue" HorizontalAlignment="Right" Margin="0,0,30,15" VerticalAlignment="Bottom" Width="135" Click="btContinue_Click" Height="25"/>

    </Grid>
</Window>
